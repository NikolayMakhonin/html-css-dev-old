{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": [
    "source/**/*"
  ],
  "exclude": [
    "node_modules/*",
    "public/*"
  ],
    "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "isolatedModules": false,
    "declaration": false,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "newLine": "LF",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "DOM",
      "ES6",
      "ES2020.Promise"
    ]
  }
}
